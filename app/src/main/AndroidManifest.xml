<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uc.portocashier">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Porto Cashier"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.uc.portocashier.model.HomeActivity"></activity>
        <activity android:name="com.uc.portocashier.directory.AdapterUser" />
        <activity android:name="com.uc.portocashier.model.SimpanData" />
        <activity android:name="com.uc.portocashier.AddUserActivity" />
        <activity android:name="com.uc.portocashier.model.User" />
        <activity android:name=".model" />
        <activity android:name="com.uc.portocashier.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>